    },
    "module2Name": {
      "type": "parameter",
      "defaultValue": "Module2",
      "replaces": "TypeEdgeModule2",
      "fileRename": "TypeEdgeModule2"
    },
    "connectionString": {
      "type": "parameter",
      "defaultValue": "PUT YOUR IOT HUB OWNER CONNECTION STRING HERE",
      "replaces": "CONNECTION_STRING"
    }
  }
}